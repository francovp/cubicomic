<link rel="stylesheet" href="~/Content/mostrarall.css" />
<br /><br /><br /><br />
<div class="row">
    <div class="col-md-2">
		<nav class="navbar navbar-inverse" role="navigation">
			<div class="innertube">
				<h3>Categorias</h3>
				<ul>
					<li><a href="#">Todos</a></li>
					<li><a href="#">Mangas</a></li>
					<li><a href="#">Comics</a></li>
					<li><a href="#">Novelas</a></li>

				</ul>
				<h3>Géneros de mangas</h3>
				<ul>
					<li><a href="#">Shōnen</a></li>
					<li><a href="#">josei</a></li>
					<li><a href="#">Kodomo</a></li>
					<li><a href="#">Seinen</a></li>
					<li><a href="#">shōjo</a></li>
				</ul>
				<h3>Géneros de comics</h3>
				<ul>
					<li><a href="#">Acción</a></li>
					<li><a href="#">Comedia</a></li>
					<li><a href="#">Terror</a></li>
					<li><a href="#">Historia de Vidas</a></li>
					<li><a href="#">Sobrenatural</a></li>
				</ul>
				<h3>Géneros de Novelas</h3>
				<ul>
					<li><a href="#">Acción</a></li>
					<li><a href="#">Comedia</a></li>
					<li><a href="#">Terror</a></li>
					<li><a href="#">Historia de Vidas</a></li>
					<li><a href="#">Sobrenatural</a></li>
				</ul>
			</div>
		</nav>
	</div>
	<div class="col-md-10">
		<div class="row">

			@foreach (var item in ViewBag.lista)
			{
				<div class="col-md-4">
					<img src="Uploads/@item" width="300" height="300" />
                    <br /><br /><br />
				</div>
			}
			@foreach (var item2 in ViewBag.muestraPDF)
			{
				<div class="col-md-4">
					<a class="viewbag-muestrapdf-mostrarall-index" href="@Url.Action(
							"Index",
							"MostrarPDF",
							new{
							busqueda = item2
						},
						null
						)">
						<img src="@Url.Content("/Uploads/"+@item2)" width="300" height="300" />
					</a>
                    <br /><br /><br />

				</div>

			}
		</div>
	</div>
</div>