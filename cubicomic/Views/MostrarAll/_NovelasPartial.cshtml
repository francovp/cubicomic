<div class="row">
	@foreach (var item2 in ViewBag.listaNovelaPartial)
	{
		<div class="col-lg-3 col-md-4 col-xs-6 thumb">
			<a class="thumbnail" href="@Url.Action(
					"Index",
					"MostrarPDF",
					new{
					busqueda = item2
				},
				null
				)">
				<img src="@Url.Content("/Uploads/"+@item2)"  class="img-responsive mostrarall-thumbnail" alt="" />
			</a>
            <br /><br /><br />

		</div>

	}
</div>

    