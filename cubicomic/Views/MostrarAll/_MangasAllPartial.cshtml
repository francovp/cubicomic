<div class="row">
	@foreach (var item4 in ViewBag.listaMangaAll)
	{
		<div class="col-lg-3 col-md-4 col-xs-6 thumb">
			<a class="thumbnail-mostrarall" href="@Url.Action(
					"Index",
					"LectorComic",
					new{
					busqueda = item4
				},
				null
				)">
				<img src="@Url.Content("/Uploads/"+@item4)" class="img-responsive mostrarall-thumbnail" alt=""  />
                <div class="image-manga">Manga</div>
			</a>
            <br /><br /><br />
		</div>
	}
</div>

    